package com.golden.ihome.crm.model;

import java.io.Serializable;
/**
 * @author dl
 * 类名称：房源联系人
 * 时间 2016-11-7
 */
public class HouseLinkManModel implements Serializable{

	private static final long serialVersionUID = 8124979435319608729L;
	
	private Integer id;
	private Integer type;//联系人类型1.业主(产权人) 2.父母 3.丈夫 4.妻子 5.子女 6.亲戚 7.其他
	private String name;//联系人姓名
	private String contactType;//联系方式
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getContactType() {
		return contactType;
	}
	public void setContactType(String contactType) {
		this.contactType = contactType;
	}
	
}
