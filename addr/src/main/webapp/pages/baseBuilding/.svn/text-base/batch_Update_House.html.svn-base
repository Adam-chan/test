<script type="text/javascript">
aihome.validateFunction=function(){
	 $.validator.addMethod("englishVerification",function(value,element,params){
			return this.optional(element) || value.length>0? 1 :  value.length;
		},"必须为英文");
	 return $("#saveForm").validate({
			onfocusout: function(element) { $(element).valid(); },  
			rules:{  
			},
			messages:{
				coveredArea:{       
					number:"必须为数字",
				},useLand:{       
					number:"必须为数字",
				},room:{
					required:"室不能为空",
				}
			},
	        errorPlacement: function(error, element){
	            error.appendTo(element.parent());
	        }
		}); 
	
}
//添加
aihome.batchUpdateBaseHouse=function(){

	var type = 0;
	// 获取表单
	var oForm = $('#saveForm');
	var saveForm = oForm.serialize();
	//表单验证
	if(!aihome.validateFunction().form()) {
		alert("添加失败！")
		type = -2;
		return type;
	}
/* 	//判断是否所有属性都为空
