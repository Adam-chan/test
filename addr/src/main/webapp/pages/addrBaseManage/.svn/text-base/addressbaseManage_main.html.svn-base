<title>地址库管理</title>
<!-- tools S -->
<div id="top_tools" class=" btn-group">
  <button id="addrBase_query" class="btn btn-sm btn-white  confirmBtn" id="confirm" hidden>
    <i class="iconfont">&#xe6e4;</i>
    <span>查询</span>
  </button>
   <button id="addrBase_Use" class="btn btn-lg btn-success" hidden onclick="aihome.updateHouse(1)">
	    <i class="ace-icon fa fa-check"></i>   
	    <span>启用</span>
  </button>
  <button id="addrBase_enUse" class="btn btn-xs btn-danger" hidden onclick="aihome.updateHouse(2)">
     <i class="ace-icon fa fa-bolt bigger-110"></i>
        <span> 禁用</span>    
   </button>
</div>
<div class="row">
	<div class="col-xs-12">
	  <div class="tabbable tabbable-lightBlue">
	    <ul class="nav nav-tabs" >
	      <li id="road_tab" class="active">
	        <a data-toggle="tab" tabUrl="${CTX}/addrBaseManage/standardAddrBaseTable.do" tabView="#tab-content1">标准地址库</a>
	      </li>
	      <!-- <li>
	        <a data-toggle="tab" tabUrl="${CTX}/addrBaseManage/temporaryAddrBaseTable.do" tabView="#tab-content1">临时地址库</a>
	      </li> -->
	    </ul>
	    <div class="tab-content" id="tab-content1"></div>
	  </div>
	</div> 
</div> 
<div id="road_form" ></div>
<script type="text/javascript">
$('.page-content-area').ace_ajax('loadScripts', [null, null], function () {
	$(window).on("resize.addrChange", function () {
		$(".addrChange .ui-jqgrid-bdiv").jqGrid("setGridHeight", $(".page-content").height() - 255);
		$(".addrChange .ui-jqgrid-btable").jqGrid("setGridWidth", $(".page-content").width() - 28);
	});
});
</script>