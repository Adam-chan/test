}
$(function () {
	$(".page-content-area .row .my-group").css("position", "relative");
	if($("#premisesId").val() != "") {
		$("[id^='buildingGroup']").attr("disabled", "disabled");
	}
	aihome.countEdu();
	aihome.countBusStation();
	aihome.countSubwayStation();
	// 基础信息 Strat
	// 楼盘类型 Start
	$("[id^='type']").change(function() {
		aihome.changeType($(this).val());
	});
	// 楼盘类型 End
	// 楼盘名称 Start
	$("[id^='name']").change(function() {
		$("[id^='name']").val($(this).val());
	});
	// 楼盘名称 End
	// 楼盘别名 Start
	$("[id^='alias']").change(function() {
		$("[id^='alias']").val($(this).val());
	});
	// 楼盘别名 End
	// 详细地址 Start
	$("[id^='addrDetail']").autocomplete({
		source : function(request, response) {
			$.ajax({
				url : "${CTX}/premisesManage/getAddrDetail.do",
				dataType : "json",
				type : 'post',
				data : {
					premisesId : $("#premisesId").val(),
					matchStr : request.term.replace(/\s/g, "")
				},
				success : function(data) {
					var array = new Array(data.length);
					var start = 0;
					if($("#oldAreaId").val() != "") {
						var temp0 = {
							areaId : $("#oldAreaId").val(),
							key : $("#oldRoadId").val(),
							value : $("#oldAddrDetail").val()
						};
						array[0] = temp0;
						start = 1;
					}
					for (var i = 0; i < data.length; i++) {
						var temp = {
							areaId : data[i].AREAID,
							key : data[i].ROADID,
							value : data[i].ADDRDETAIL
						};
						array[i+start] = temp;
					}
					response(array);
				}
			});
		},
		select : function(event, ui) {
			$("[id$='areaId']").val(ui.item.areaId);
			$("[id^='roadId']").val(ui.item.key);
			$("[id^='addrDetail']").val(ui.item.value);
			event.preventDefault();
			aihome.getParentBusiArea(ui.item.areaId, "busiAreaParentId");
			aihome.cleanOption("busiAreaId");
			aihome.getSubdistrict(ui.item.areaId, "subdistrictId");
			aihome.cleanOption("communityId");
		}
	});
	$("[id^='addrDetail']").blur(function() {
		$(this).attr("placeholder", "");
		if($("#addrDetail").val() == "") {
			aihome.getParentBusiArea("", "busiAreaParentId");
			aihome.cleanOption("busiAreaId");
			aihome.getSubdistrict("", "subdistrictId");
			aihome.cleanOption("communityId");
		}
	});
	// 详细地址 End
	// 商圈 Start
	$("[id$='busiAreaParentId']").change(function() {
		$("[id$='busiAreaParentId']").val($(this).val());
		// 大商圈 -> 小商圈(级联) Start
		aihome.getBusiArea($(this).val(), $("#areaId").val(), "busiAreaId");
		// 大商圈 -> 小商圈(级联) End
