<script type="text/javascript">
aihome.type = 1;
aihome.eduIdStr = "";
aihome.busStationIdStr = "";
aihome.subwayStationIdStr = "";
/* 行政区 -> 大商圈(级联) */
aihome.getParentBusiArea = function(areaId, objId) {
	$.ajax({
		type : "post",
		url : "${CTX}/busiArea/getParentBusiAreaList.do", 
		async : false,
		dataType : "json",
		data : {"areaId" : areaId},
		success : function(data) {
			aihome.changeOption(data, objId);
		},
	});
}
/* 大商圈 -> 小商圈(级联) */
aihome.getBusiArea = function(busiAreaParentId, areaId, objId) {
	$.ajax({
		type : "post",
		url : "${CTX}/busiArea/getBusiAreaList.do", 
		async : false,
		dataType : "json",
		data : {"busiAreaParentId" : busiAreaParentId, "areaId" : areaId},
		success : function(data) {
			aihome.changeOption(data, objId);
		},
	});
}
/* 行政区 -> 街道(级联) */
aihome.getSubdistrict = function(areaId, objId) {
	aihome.cleanOption("communityId");
	$.ajax({
		type : "post",
		url : "${CTX}/area/getSubdistrictList.do", 
		async : false,
		dataType : "json",
		data : {"baseAreaId" : areaId},
		success : function(data) {
			aihome.changeOption(data, objId);
		},
	});
}
/* 街道 -> 社区(级联) */
aihome.getCommunity = function(subdistrictId, objId) {
	$.ajax({
		type : "post",
		url : "${CTX}/area/getCommunityList.do", 
		async : false,
		dataType : "json",
		data : {"subdistrictId" : subdistrictId},
		success : function(data) {
			aihome.changeOption(data, objId);
		},
	});
}
/* 级联改变下拉框内容 */
aihome.changeOption = function (data, objId) {
	aihome.cleanOption(objId);
	var str = "";
	for(var i=0; i < data.length; i++) {
		var id = data[i].id
		var name = data[i].name == undefined ? data[i].busiAreaName : data[i].name;
		var optionShow = $("[id$='" + objId + "']");
		str = "<option value=\"" + id + "\">" + name + "</option>";
		optionShow.append(str);
	}
}
/* 清空下拉框内容 */
aihome.cleanOption = function(objId) {
	$("#addFormTypeOne").find("#" + objId).children().first().siblings().remove();
	$("#addFormTypeTwo").find("#" + objId).children().first().siblings().remove();
	$("#addFormTypeThree").find("#" + objId).children().first().siblings().remove();
}
/* 楼盘类型 */
aihome.changeType = function(value) {
	aihome.type = value;
	if(value == 1) {
		$("#addFormTypeOne").show();
		$("#addFormTypeTwo").hide();
		$("#addFormTypeThree").hide();
		$("#addFormTypeOne").find("#type").val(value);
	} else if(value == 2) {
		$("#addFormTypeOne").hide();
		$("#addFormTypeTwo").show();
		$("#addFormTypeThree").hide();
		$("#addFormTypeTwo").find("#type").val(value);
	} else if(value == 3) {
		$("#addFormTypeOne").hide();
		$("#addFormTypeTwo").hide();
		$("#addFormTypeThree").show();
		$("#addFormTypeThree").find("#type").val(value);
	}
}
/* 是否有物业公司 */
aihome.changePropertyManagement = function(value) {
	if(value == 1) {
		$(".has").show();
		$(".hasNot").hide();
		$("[id^='hasPropertyManagementCompany'][value=1]").prop("checked", "checked");
		$("[id^='hasPropertyManagementCompany'][value=2]").removeAttr("checked");
	} else {
		$(".has").hide();
		$(".hasNot").show();
		$("[id^='hasPropertyManagementCompany'][value=1]").removeAttr("checked");
		$("[id^='hasPropertyManagementCompany'][value=2]").prop("checked", "checked");
	}
}
/* 统计教育设施 */
aihome.countEdu = function() {
	aihome.eduIdStr = "";
	var eduIds = $("#addFormTypeOne").find("[id^='eduIds']");
	for(var i = 0; i < eduIds.length; i++) {
		aihome.eduIdStr += eduIds[i].value + ",";
	}
	aihome.eduIdStr = aihome.eduIdStr.substring(0,aihome.eduIdStr.length-1);
}
/* 统计公交站点 */
aihome.countBusStation = function() {
	aihome.busStationIdStr = "";
	var busStationIds = $("#addFormTypeOne").find("[id^='busStationIds']");
	for(var i = 0; i < busStationIds.length; i++) {
		aihome.busStationIdStr += busStationIds[i].value + ",";
	}
	aihome.busStationIdStr = aihome.busStationIdStr.substring(0,aihome.busStationIdStr.length-1);
}
/* 统计地铁站点 */
aihome.countSubwayStation = function() {
	aihome.subwayStationIdStr = "";
	var subwayStationIds = $("#addFormTypeOne").find("[id^='subwayStationIds']");
	for(var i = 0; i < subwayStationIds.length; i++) {
		aihome.subwayStationIdStr += subwayStationIds[i].value + ",";
	}
	aihome.subwayStationIdStr = aihome.subwayStationIdStr.substring(0,aihome.subwayStationIdStr.length-1);
}
/* 关闭SPAN按钮 */
aihome.closeSpan = function(obj) {
	var id = $(obj).parent().parent().attr("id");
	var objs = $("[id^='" + $(obj).parent().next().attr("id") + "'][value='" + $(obj).parent().next().val() +"']");
	objs.prev().remove();
	objs.remove();
	if(id.indexOf("eduLabel") != -1) {
		aihome.countEdu();
	} else if(id.indexOf("busStationLabel") != -1) {
		aihome.countBusStation();
	} else if(id.indexOf("subwayStationLabel") != -1) {
		aihome.countSubwayStation();
	}
}
/* 验证电话号 */
aihome.checkPhone = function(value) {
	var isPhone = /^([0-9]{3,4}-)?[0-9]{7,8}$/;
	var isMob = /^((\+?86)|(\(\+86\)))?(13[012356789][0-9]{8}|15[012356789][0-9]{8}|18[02356789][0-9]{8}|147[0-9]{8}|1349[0-9]{7})$/;
	if(!isMob.test(value.trim()) && !isPhone.test(value.trim())) {
		return false;
	}
	return true;
}
/* 新增 */
aihome.savePremises = function() {
	var submitUrl = "${CTX}/premisesManage/addPremises.do";
	aihome.submitFunc(submitUrl);
}
/* 修改 */
aihome.updPremises = function() {
	var submitUrl = "${CTX}/premisesManage/updatePremises.do";
	aihome.submitFunc(submitUrl);
}
/* 提交审核 */
aihome.auditingPremises = function() {
	if(confirm("是否提交审核？")) {
		$.ajax({
			url : "${CTX}/premisesManage/auditingPremises.do",
			dataType : "json",
			data : {
				premisesId : $("#premisesId").val()
			},
			success : function(data) {
				alert("提交审核成功！");
			}
		});
	};
}
aihome.submitFunc = function(submitUrl) {
	if($("#name").val() == "") {
		alert("请填写楼盘名称！");
		return; 
	}
	if($("#addrDetail").val() == "") {
		alert("请填写详细地址！");
		return; 
	}
	if($("#contactsPhone").val() != "" && !aihome.checkPhone($("#contactsPhone").val())) {
		alert("请输入正确的联系电话！");
		return;
	}
	// 生活配套
	var livingFacilities = $("#addFormTypeOne").find("[id^='livingFacilities']:checked");
	var livingFacilitiesStr = "";
	for(var i = 0; i < livingFacilities.length; i++) {
		livingFacilitiesStr += livingFacilities[i].value + ",";
	}
	livingFacilitiesStr = livingFacilitiesStr.substring(0,livingFacilitiesStr.length-1);
	// 社区安全
	var communitySafe = $("#addFormTypeOne").find("[id^='communitySafe']:checked");
	var communitySafeStr = "";
	for(var i = 0; i < communitySafe.length; i++) {
		communitySafeStr += communitySafe[i].value + ",";
	}
	communitySafeStr = communitySafeStr.substring(0,communitySafeStr.length-1);
	// 小区热点分类（租赁）
	var hotSpotClassificationLease = $("#addFormTypeOne").find("[id^='hotSpotClassificationLease']:checked");
	var hotSpotClassificationLeaseStr = "";
	for(var i = 0; i < hotSpotClassificationLease.length; i++) {
		hotSpotClassificationLeaseStr += hotSpotClassificationLease[i].value + ",";
	}
	hotSpotClassificationLeaseStr = hotSpotClassificationLeaseStr.substring(0,hotSpotClassificationLeaseStr.length-1);
	// 小区热点分类（买卖）
	var hotSpotClassificationSales = $("#addFormTypeOne").find("[id^='hotSpotClassificationSales']:checked");
	var hotSpotClassificationSalesStr = "";
	for(var i = 0; i < hotSpotClassificationSales.length; i++) {
		hotSpotClassificationSalesStr += hotSpotClassificationSales[i].value + ",";
	}
	hotSpotClassificationSalesStr = hotSpotClassificationSalesStr.substring(0,hotSpotClassificationSalesStr.length-1);
	$.ajax({
		type : "post",
		url : submitUrl,
		async : false,
		dataType : 'json',
		data : {
			"premisesId" : $("#premisesId").val(),
			"type" : aihome.type,
			"name" : $("#name").val(),
			"alias" : $("#alias").val(),
			"roadId" : $("#roadId").val(),
			"busiAreaId" : $("#busiAreaId").val() == "" ? $("#busiAreaParentId").val() : $("#busiAreaId").val(),
			"subdistrictId" : $("#subdistrictId").val(),
			"communityId" : $("#communityId").val(),
			"alternateAddress" : $("#alternateAddress").val(),
			"buildingGroup" : $("#buildingGroup").val(),
			"buildingAge" : $("#buildingAge").val(),
			"housingPurposes" : $("#housingPurposes").val(),
			"landUsageTerm" : $("#landUsageTerm").val(),
			"buildingStructure" : $("#buildingStructure").val(),
			"buildingType" : $("#buildingType").val(),
			"housingProperty" : $("#housingProperty").val(),
			"regionMeasure" : $("#regionMeasure").val(),
			"areaCovered" : $("#areaCovered").val(),
			"overallFloorage" : $("#overallFloorage").val(),
			"plotRatio" : $("#plotRatio").val(),
			"greeningRate" : $("#greeningRate").val(),
			"parkingNum" : $("#parkingNum").val(),
			"maintenanceCategory" : $("#maintenanceCategory").val(),
			"housingType" : $("#housingType").val(),
			"startingAtPrice" : $("#startingAtPrice").val(),
			"averagePrice" : $("#averagePrice").val(),
			"developer" : $("#developer").val(),
			"builder" : $("#builder").val(),
			"agent" : $("#agent").val(),
			"hasPropertyManagementCompany" : $("[id^='hasPropertyManagementCompany']:checked").val(),
			"propertyManagementCompany" : $("#propertyManagementCompany").val(),
			"companyAddress" : $("#companyAddress").val(),
			"propertyFee" : $("#propertyFee").val(),
			"cleaningFee" : $("#cleaningFee").val(),
			"busStation" : aihome.busStationIdStr,
			"subwayStation" : aihome.subwayStationIdStr,
			"openingTime" : $("#openingTime").val(),
			"checkInTime" : $("#checkInTime").val(),
			"agentType" : $("#agentType").val(),
			"salesStartDate" : $("#salesStartDate").val(),
			"salesEndDate" : $("#salesEndDate").val(),
			"commissionRate" : $("#commissionRate").val(),
			"expectEarnings" : $("#expectEarnings").val(),
			"tradedHouseholds" : $("#tradedHouseholds").val(),
			"contacts" : $("#contacts").val(),
			"contactsPhone" : $("#contactsPhone").val(),
			"salesAddress" : $("#salesAddress").val(),
			"permitForPresale" : $("#permitForPresale").val(),
			"supportingFacilities" : $("#supportingFacilities").val(),
			"houseType" : $("#houseType").val(),
			"totalBuildings" : $("#totalBuildings").val(),
			"totalApartments" : $("#totalApartments").val(),
			"status" : $("#status").val(),
			"eduIds" : aihome.eduIdStr,
			"livingFacilities" : livingFacilitiesStr,
			"communitySafe" : communitySafeStr,
			"hotSpotClassificationLease" : hotSpotClassificationLeaseStr,
			"hotSpotClassificationSales" : hotSpotClassificationSalesStr,
			"premisesProfile" : $("#premisesProfile").val(),
			"farmProductMarket" : $("#farmProductMarket").val(),
			"supermarket" : $("#supermarket").val(),
			"entertainment" : $("#entertainment").val(),
			"bankParkHospital" : $("#bankParkHospital").val(),
			"remark" : $("#remark").val()
			},
		success : function(data){
			if(data != 0) {
				$("#premisesId").val(data);
				$(".page-content-area").prop("scrollTop", 0);
				oaDialogOpen(
						"returnLayout",//对话框ID，必填，且不能与当前页面的ID重复
