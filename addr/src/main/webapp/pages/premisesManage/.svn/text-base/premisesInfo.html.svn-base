							url: "${CTX}/premisesManage/returnLayout.do?premisesId="+$("#premisesId").val()+"&premisesName="+$("#name").val()+"&areaId="+$("#areaId").val(),//调用页面的url
							title: "",//弹框的title
							closeOnEscpe: false,//是否通过esc关闭弹框
							closeBtn: false,//是否显示关闭按钮
							width: 309,//弹框的宽度
							height: 200,//弹框的高度
							openCall: function() {
								//弹框打开后的回调函数
								$("#returnLayout").prev().css("cssText", "background: #FFFFFF !important; border-bottom: 0;");
