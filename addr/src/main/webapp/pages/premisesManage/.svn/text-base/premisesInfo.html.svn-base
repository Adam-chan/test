	// 商圈 End
	// 小商圈 Start
	$("[id$='busiAreaId']").change(function() {
		$("[id$='busiAreaId']").val($(this).val());
	});
	// 小商圈 End
	// 街道 Start
	$("[id^='subdistrictId']").change(function() {
		$("[id^='subdistrictId']").val($(this).val());
		// 街道 -> 社区(级联) Start
		aihome.getCommunity($(this).val(), "communityId");
		// 街道 -> 社区(级联) End
	});
	// 街道 End
	// 社区 Start
	$("[id^='communityId']").change(function() {
		$("[id^='communityId']").val($(this).val());
	});
	// 社区 End
	// 备用地址 Start
	$("[id^='alternateAddress']").change(function() {
		$("[id^='alternateAddress']").val($(this).val());
	});
	// 备用地址 End
	// 楼栋分组 Start
	$("[id^='buildingGroup']").change(function() {
		$("[id^='buildingGroup']").val($(this).val());
	});
	// 楼栋分组 End
	// 起价 Start
	$("[id^='startingAtPrice']").change(function() {
		$("[id^='startingAtPrice']").val($(this).val());
	});
	// 起价 End
	// 均价 Start
	$("[id^='averagePrice']").change(function() {
		$("[id^='averagePrice']").val($(this).val());
	});
	// 均价 End
	// 区域面积 Start
	$("[id$='regionMeasure']").change(function() {
		$("[id$='regionMeasure']").val($(this).val());
	});
	// 区域面积 End
	// 房屋用途 Start
	$("[id$='housingPurposes']").change(function() {
		$("[id$='housingPurposes']").val($(this).val());
	});
	// 房屋用途 End
	// 土地使用年限 Start
	$("[id^='landUsageTerm']").change(function() {
		$("[id^='landUsageTerm']").val($(this).val());
	});
	// 土地使用年限 End
	// 建筑结构 Start
	$("[id^='buildingStructure']").change(function() {
		$("[id^='buildingStructure']").val($(this).val());
	});
	// 建筑结构 End
	// 建筑类型 Start
	$("[id^='buildingType']").change(function() {
		$("[id^='buildingType']").val($(this).val());
