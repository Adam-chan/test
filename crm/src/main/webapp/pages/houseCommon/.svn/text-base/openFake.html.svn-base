<script type="text/javascript">
$(":input[name='fakeType']").change(function (){
	aihome.fakeType = $('input[name="fakeType"]:checked').val();
	if($(this).val()==1){
		$("#key_house").hide();
		$("#porxy_house").hide();
		$("#fake_house").show();
	}else if($(this).val()==2){
		
	}else if($(this).val()==3){
		$("#fake_house").hide();
		$("#porxy_house").hide();
		$("#key_house").show();
	}else{
		$("#porxy_house").show();
		$("#key_house").hide();
		$("#fake_house").hide();
	}
});
</script>
