<!-- 页面描述:租赁房源(商铺) -->
<div class="panel">
	<form id="house_lease_Form">
		<div class="panel-heading" style="background-color: #A8C9F4;">
			<h4 class="panel-title">
				<a class="accordion-toggle">基本信息</a>
			</h4>
		</div>
		<div class="panel-collapse">
			<div class="panel-body">
				<div class="modal-body form-horizontal row" >
					<div class="col-sm-12 form-xs form-group row" style="left:-5%">
						<#include "/houseLease/addrDetail.html"/>
					</div>
					<div class="col-sm-12 form-xs form-group row" style="left:-5.5%">
						 <label class="col-xs-2 control-label">户型：</label>
				         <div class="col-xs-1">
				            <input id="rooms" type="text" class="form-control input-xs checkNum" format="2" value="${(houseLeaseResult.rooms)!''}" name="rooms" style="width: 110%;">
				         </div>
				         <div class="col-xs-1">室</div>
				         <div class="col-xs-1" style="left: -6%;">
				            <input id="office" type="text" class="form-control input-xs checkNum" format="2" value="${(houseLeaseResult.office)!''}" name="office" style="width: 110%;">
				         </div>
				         <div class="col-xs-1" style="left: -6%;">厅</div>
				         <div class="col-xs-1" style="left: -12%;">
				            <input id="kitchen" type="text" class="form-control input-xs checkNum" format="2" value="${(houseLeaseResult.kitchen)!''}" name="kitchen" style="width: 110%;">
				         </div>
				         <div class="col-xs-1" style="left: -12%;">厨</div>
				         <div class="col-xs-1" style="left: -18%;">
				            <input id="wc" type="text" class="form-control input-xs checkNum" format="2" value="${(houseLeaseResult.wc)!''}" name="wc" style="width: 110%;">
				         </div>
				         <div class="col-xs-1" style="left: -18%;">卫</div>
				    </div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">使用类别：</label>
							<div class="col-xs-7">
								<select id="house_useType" class="input-xs form-control" name="house_useType" disabled="true">
									<option value="1">住宅</option>
									<option value="2" selected="selected">商铺</option>
									<option value="3">写字楼</option>
									<option value="4">车位</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">楼型：</label>
					        <div class="col-xs-7">
					             <select id="buildingType" class="input-xs form-control" name="buildingType">
					             	  <option value="">请选择</option>
					                  <option value="1">高层</option>
									  <option value="2">多层</option>
									  <option value="3">平房</option>
									  <option value="4">别墅</option>
									  <option value="5">其它</option>
					             </select>
					        </div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">建筑面积：</label>
							<div class="col-xs-7">
								<input id="coveredArea" type="text" class="form-control input-xs checkNum" format="7.2" name="coveredArea" value="${(houseLeaseResult.coveredArea)!''}">
							</div>
							<div class="col-xs-1">㎡</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">使用面积：</label>
							<div class="col-xs-7">
								<input id="usedArea" type="text" class="form-control input-xs checkNum" format="7.2" name="usedArea" value="${(houseLeaseResult.usedArea)!''}">
							</div>
							<div class="col-xs-1">㎡</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">意向租价：</label>
							<div class="col-xs-7">
								<input id="leasePrice" type="text" class="form-control input-xs checkNum" format="10.2" name="leasePrice" value="${(houseLeaseResult.leasePrice)!''}">
							</div>
							<div class="col-xs-2">元/月</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">物业费：</label>
					        <div class="col-xs-7">
								<input id="propertyFee" type="text" class="form-control input-xs checkNum" format="10.2" name="propertyFee" value="${(houseLeaseResult.propertyFee)!''}"> 
							</div> 
							<div class="col-xs-2">元/月/㎡</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">临近位置：</label>
					        <div class="col-xs-7">
					       	 	<input id="closePosition" type="text" class="form-control input-xs" name="closePosition" value="${(houseLeaseResult.closePosition)!''}"> 
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">使用现状：</label>
					        <div class="col-xs-7">
								<select id="useStatus" class="input-xs form-control" name="useStatus">
									<option value="">请选择</option>
									<option value="1">空置</option>
									<option value="2">出租</option>
									<option value="3">自住</option>
								</select>
							</div> 
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">房源状态：</label>
							<div class="col-xs-7">
								<select id="status" class="input-xs form-control" name="status" disabled="true">
									<option value="1" selected="selected">待租</option>
								</select>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="panel-heading" style="background-color: #A8C9F4;">
			<h4 class="panel-title">
				<a class="accordion-toggle">
					其他信息
				</a>
			</h4>
		</div>
		<div class="panel-collapse">
			<div class="panel-body">
				<div class="modal-body form-horizontal row" style="left: -70px;">
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">业主来源渠道：</label>
						<div class="col-xs-2">
							<select id="parentSource" class="floor input-xs form-control" onchange="aihome.getChildrenSource(this);">
								<option value="">请选择</option>
								<#if parentSourceList?exists>
									<#list parentSourceList as source>
										<option value="${source.id!''}">
											${source.source!''}
										</option>
									</#list>
								</#if>
							</select>
						</div>
						<div class="col-xs-2">
							<select id="childSource" class="floor input-xs form-control" name="childSource">
								<option value="">请选择</option>
							</select>
						</div>
					</div>
					<!-- 看房方式 s -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">看房方式：</label>
						<#if sysParamMap.ZLKFFS??>
							<#list sysParamMap.ZLKFFS as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="lookWay ace" value="${item.id!''}">
										<span class="lbl"> ${item.sysHouseDescribe!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list> 
					    </#if>  
					</div>
					<!-- 看房方式 e -->
					<!-- 适合经营 S -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">适合经营：</label>
						<#if sysParamMap.ZLSHJY??>
							<#list sysParamMap.ZLSHJY as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="fitManagement ace" value="${item.id!''}">
										<span class="lbl"> ${item.sysHouseDescribe!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>  
					    </#if> 
					</div>
					<!-- 适合经营 E -->
					<!-- 房源描述 s -->
					<div class="col-sm-12 form-xs form-group row">
						<!-- 房源描述row1 -->
						<label class="col-xs-2 control-label">房源描述：</label>
					    <#if sysParamMap.ZLFYMS??>
							<#list sysParamMap.ZLFYMS as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="special ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>   
					    </#if>
					</div>
					<!-- 房源描述 e -->
					<!-- 房源标签 s -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">房源标签：</label>
						<#if sysParamMap.ZLFYBQ??>
							<#list sysParamMap.ZLFYBQ as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="tags ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>
					    </#if> 
					</div>
					<!-- 房源标签 e -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">备注：</label>
						<div class="col-xs-10">
							<textarea id="remark" rows="5" cols="25" class="form-control" maxlength="300">${(houseLeaseResult.remark)!''}</textarea>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- 隐藏域 S -->
		<input type="hidden" name="houseInfoId" value="${(houseLeaseResult.houseInfoId)!''}">
		<input type="hidden" name="oldLeasePrice" value="${(houseLeaseResult.leasePrice)!''}"><!-- 意向租金 ：旧 -->
		<!-- 隐藏域 E -->
	</form>
</div>

<script type="text/javascript">
	
	$(function(){

		//修改-数据初始化
		var buildingType = "${(houseLeaseResult.buildingType)!''}";//楼型
		if(buildingType!=""){
			$("#buildingType").find("[value='"+buildingType+"']").attr("selected", "selected");
		}
		var useStatus = "${(houseLeaseResult.useStatus)!''}";//使用现状
		if(useStatus!=""){
			$("#useStatus").find("[value='"+useStatus+"']").attr("selected", "selected");
		}
		var source1 = "${(houseLeaseResult.source1)!''}";//业主来源
		if(source1!=""){
			aihome.getChildrenSource(source1);
			$("#parentSource").find("[value='"+source1+"']").attr("selected", "selected");//业主来源1
			var source2 = "${(houseLeaseResult.source2)!''}";//业主来源2
			if(source2!=""){
				$("#childSource").find("[value='"+source2+"']").attr("selected", "selected");
			}
		}
		var lookWay = "${(houseLeaseResult.lookWay)!''}";//看房方式
		if(lookWay!=""){
			var lookWayArray = lookWay.split(",");
			$(lookWayArray).each(function(i){
				$(".lookWay[value='"+lookWayArray[i]+"']").prop("checked", "checked");
			});
		}
		var fitManagement = "${(houseLeaseResult.fitManagement)!''}";//适合经营
		if(fitManagement!=""){
			var fitManagementArray = fitManagement.split(",");
			$(fitManagementArray).each(function(i){
				$(".fitManagement[value='"+fitManagementArray[i]+"']").prop("checked", "checked");
			});
		}
		var special = "${(houseLeaseResult.special)!''}";//房源描述
		if(special!=""){
			var specialArray = special.split(",");
			$(specialArray).each(function(i){
				$(".special[value='"+specialArray[i]+"']").prop("checked", "checked");
			});
		}
		var tags = "${(houseLeaseResult.tags)!''}";//房源标签
		if(tags!=""){
			var tagsArray = tags.split(",");
			$(tagsArray).each(function(i){
				$(".tags[value='"+tagsArray[i]+"']").prop("checked", "checked");
			});
		}
	});
	
</script>
			
			
			
			
			
			
			
			