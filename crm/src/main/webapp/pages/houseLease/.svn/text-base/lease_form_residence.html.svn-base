<!-- 页面描述:租赁房源(住宅) -->
<div class="panel">
	<form id="house_lease_Form">
		<div class="panel-heading" style="background-color: #A8C9F4;">
			<h4 class="panel-title">
				<a class="accordion-toggle">基本信息</a>
			</h4>
		</div>
		<div class="panel-collapse">
			<div class="panel-body">
				<div class="modal-body form-horizontal row" >
					<div class="col-sm-12 form-xs form-group row" style="left:-5%">
						<#include "/houseLease/addrDetail.html"/>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">使用类别：</label>
							<div class="col-xs-8">
								<select id="house_useType" class="input-xs form-control" name="house_useType" disabled="true">
									<option value="1" selected="selected">住宅</option>
									<option value="2">商铺</option>
									<option value="3">写字楼</option>
									<option value="4">车位</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">房屋性质：</label>
							<div class="col-xs-8">
								<select id="property" class="input-xs form-control" name="property">
									<option value="">请选择</option>
									<option value="1">房改房</option>
									<option value="2">商品房</option>
									<option value="3">回迁房</option>
									<option value="4">经济适用房</option>
								</select>
							</div>
						</div>
						<div id="property_right_div" class="col-sm-4" style="left:-4%" hidden>
							<div class="col-xs-8">
								<select id="propertyRight" class="input-xs form-control" name="propertyRight">
									<option value="">请选择</option>
									<option value="1">已变更产权</option>
									<option value="2">未变更产权</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4" style="left:2.6%;">
							<label class="col-xs-3 control-label" style="left: 0%;">户型：</label>
							<div class="col-xs-1" style="width: 13%;">
								<input id="rooms" type="text" class="form-control input-xs checkNum" format="2" name="rooms" value="${(houseLeaseResult.rooms)!''}">
							</div>
							<div class="col-xs-1" style="left: -3%;width: 1;">室</div>
							<div class="col-xs-1" style="width: 13%;left: -7%;">
								<input id="office" type="text" class="form-control input-xs checkNum" format="2" name="office" value="${(houseLeaseResult.office)!''}">
							</div>
							<div class="col-xs-1" style="left: -10%;width: 1%;">厅</div>
							<div class="col-xs-1" style="width: 13%;left: -10%;">
								<input id="kitchen" type="text" class="form-control input-xs checkNum" format="2" name="kitchen" value="${(houseLeaseResult.kitchen)!''}">
							</div>
							<div class="col-xs-1" style="left: -13%;width: 1%;">厨</div>
							<div class="col-xs-1" style="width: 13%;left: -13%;">
								<input id="wc" type="text" class="form-control input-xs checkNum" format="2" name="wc" value="${(houseLeaseResult.wc)!''}">
							</div>
							<div class="col-xs-1" style="width: 1%;left: -16%;">卫</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">建筑面积：</label>
							<div class="col-xs-8">
								<input id="coveredArea" type="text" class="form-control input-xs checkNum" format="7.2" name="coveredArea" value="${(houseLeaseResult.coveredArea)!''}">
							</div>
							<div class="col-xs-1">㎡</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">使用面积：</label>
							<div class="col-xs-8">
								<input id="usedArea" type="text" class="form-control input-xs checkNum" format="7.2" name="usedArea" value="${(houseLeaseResult.usedArea)!''}">
							</div>
							<div class="col-xs-1">㎡</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">楼型：</label>
							<div class="col-xs-8">
								<select id="buildingType" class="input-xs form-control" name="buildingType">
									<option value="">请选择</option>
									<option value="1">高层</option>
									<option value="2">多层</option>
									<option value="3">平房</option>
									<option value="2">别墅</option>
									<option value="3">其他</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">建筑年代：</label>
							<div class="col-xs-8">
								<input id="buildTime" type="text" class="form-control input-xs checkNum" format="4" name="buildTime" value="${(houseLeaseResult.buildTime)!''}">
							</div>
							<div class="col-xs-1">年</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">使用现状：</label>
							<div class="col-xs-8">
								<select id="useStatus" class="input-xs form-control" name="useStatus">
									<option value="">请选择</option>
									<option value="1">空置</option>
									<option value="2">出租</option>
									<option value="3">自住</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">装修状况/年限：</label>
							<div class="col-xs-4">
								<select id="decoradion" class="input-xs form-control" name="decoradion">
									<option value="">请选择</option>
									<option value="1">毛坯</option>
									<option value="2">简装</option>
									<option value="3">精装</option>
									<option value="4">豪装</option>
									<option value="5">中等</option>
								</select>
							</div>
							<div class="col-xs-4">
								<select id="decoradionTime" class="input-xs form-control" name="decoradionTime">
									<option value="">请选择</option>
									<option value="1">半年以内</option>
									<option value="2">一年以内</option>
									<option value="3">一年至三年</option>
									<option value="4">三年以上</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">出租形式：</label>
							<div class="col-xs-8">
								<select id="leaseType" class="input-xs form-control" name="leaseType">
									<option value="">请选择</option>
									<option value="1">整租</option>
									<option value="2">分租</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">朝向：</label>
							<div class="col-xs-8">
								<select id="chaoXiang" class="input-xs form-control" name="chaoXiang">
									<option value="">请选择</option>
									<option value="1">东</option>
									<option value="2">南</option>
									<option value="3">西</option>
									<option value="4">北</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">意向租金：</label>
							<div class="col-xs-6">
								<input id="leasePrice" type="text" class="form-control input-xs checkNum" format="10.2" name="leasePrice" value="${(houseLeaseResult.leasePrice)!''}">
							</div>
							<div class="col-xs-2">元/月</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">至少租期：</label>
							<div class="col-xs-8">
								<input id="shortestLease" type="text" class="form-control input-xs checkNum" format="4" name="shortestLease" value="${(houseLeaseResult.shortestLease)!''}">
							</div>
							<div class="col-xs-1">月</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">押金要求：</label>
							<div class="col-xs-8">
								<input id="depositWay" type="text" class="form-control input-xs checkNum" format="10.2" name="depositWay" value="${(houseLeaseResult.depositWay)!''}">
							</div>
							<div class="col-xs-1">元</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">付款方式：</label>
							<div class="col-xs-8">
								<select id="payType" class="input-xs form-control" name="payType">
									<option value="">请选择</option>
									<option value="1">季付</option>
									<option value="2">月付</option>
									<option value="3">日付</option>
									<option value="4">半年付</option>
									<option value="5">年付</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">腾空时间：</label>
							<div class="col-xs-8">
								<select id="takeEmpty" class="input-xs form-control" name="takeEmpty">
									<option value="">请选择</option>
									<option value="1">签约一周后</option>
									<option value="2">签约10天内</option>
									<option value="3">签约20天内</option>
								</select>
							</div>
						</div>
						<div class="col-sm-4">
							<label class="col-xs-3 control-label">楼层：</label>
							<div class="col-xs-8">
								<select id="floorType" class="input-xs form-control" name="floorType">
									<option value="">请选择</option>
									<option value="1">低楼层</option>
									<option value="2">中楼层</option>
									<option value="3">高楼层</option>
								</select>
							</div>
						</div>
					</div>
					<div class="col-sm-12 form-xs form-group row">
						<div class="col-sm-4">
							<label class="col-xs-4 control-label">房源状态：</label>
							<div class="col-xs-8">
								<select id="status" class="input-xs form-control" name="status" disabled="true">
									<option value="1" selected="selected">待租</option>
								</select>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="panel-heading" style="background-color: #A8C9F4;">
			<h4 class="panel-title">
				<a class="accordion-toggle">其他信息</a>
			</h4>
		</div>
		<div class="panel-collapse">
			<div class="panel-body">
				<div class="modal-body form-horizontal row" style="left: -70px;">
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">业主来源渠道：</label>
						<div class="col-xs-2">
							<select id="parentSource" class="floor input-xs form-control" onchange="aihome.getChildrenSource(this);">
								<option value="">请选择</option>
								<#if parentSourceList?exists>
									<#list parentSourceList as source>
										<option value="${source.id!''}">
											${source.source!''}
										</option>
									</#list>
								</#if>
							</select>
						</div>
						<div class="col-xs-2">
							<select id="childSource" class="floor input-xs form-control" name="childSource">
								<option value="">请选择</option>
							</select>
						</div>
					</div>
					<!-- 看房方式 s -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">看房方式：</label>
						<#if sysParamMap.ZLKFFS??>
							<#list sysParamMap.ZLKFFS as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="lookWay ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list> 
						</#if> 
					</div>
					<!-- 看房方式 e -->
					<!-- 房屋设置 s -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">房屋设施：</label>
						<#if sysParamMap.ZLFWSS??>
							<#list sysParamMap.ZLFWSS as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="fwsh ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>  
					    </#if> 
					</div>
					<!-- 房屋设施 e -->
					<!-- 室内家具 s -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">室内家具：</label>
						<#if sysParamMap.ZLSNJJ??>
							<#list sysParamMap.ZLSNJJ as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="furniture ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>  
					    </#if>
					</div>
					<!-- 室内家具 e -->
					<!-- 房源描述 s -->
					<div class="col-sm-12 form-xs form-group row">
						<!-- 房源描述row1 -->
						<label class="col-xs-2 control-label">房源描述：</label>
						<#if sysParamMap.ZLFYMS??>
							<#list sysParamMap.ZLFYMS as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="special ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>   
					    </#if>
					</div>
					<!-- 房源描述 e -->
					<!-- 房源标签 s -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">房源标签：</label>
						<#if sysParamMap.ZLFYBQ??>
							<#list sysParamMap.ZLFYBQ as item>  
						        <div class="col-xs-2">
									<label>
										<input type="checkbox" class="tags ace" value="${item.id!''}">
										<span class="lbl"> ${(item.sysHouseDescribe)!''}</span>
									</label>
								</div>
								<#if (item_index+1)%5==0>
									<label class="col-xs-2 control-label"></label>
								</#if> 
						    </#list>
					    </#if> 
					</div>
					<!-- 房源标签 e -->
					<div class="col-sm-12 form-xs form-group row">
						<label class="col-xs-2 control-label">备注：</label>
						<div class="col-xs-10">
							<textarea id="remark" rows="5" cols="25" class="form-control" maxlength="300">${(houseLeaseResult.remark)!''}</textarea>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- 隐藏域 S -->
		<input type="hidden" name="houseInfoId" value="${(houseLeaseResult.houseInfoId)!''}">
		<input type="hidden" name="oldLeasePrice" value="${(houseLeaseResult.leasePrice)!''}"><!-- 意向租金 ：旧 -->
		<!-- 隐藏域 E -->
	</form>
</div>

<script type="text/javascript">
	
	$(function(){
		//显示产权性质
		$("#property").change(function(){
			var property = $("#property option:selected").val();
			if(property==4){
				$("#property_right_div").show();
			}else{
				$("#property_right_div").hide();
			}
		});
		
		//修改-数据初始化
		var property = "${(houseLeaseResult.property)!''}";//房屋性质
		if(property!=""){
			$("#property").find("[value='"+property+"']").attr("selected", "selected");
		}
		var floorType = "${(houseLeaseResult.floorType)!''}";//楼层
		if(floorType!=""){
			$("#floorType").find("[value='"+floorType+"']").attr("selected", "selected");
		}
		var buildingType = "${(houseLeaseResult.buildingType)!''}";//楼型
		if(buildingType!=""){
			$("#buildingType").find("[value='"+buildingType+"']").attr("selected", "selected");
		}
		var useStatus = "${(houseLeaseResult.useStatus)!''}";//使用现状
		if(useStatus!=""){
			$("#useStatus").find("[value='"+useStatus+"']").attr("selected", "selected");
		}
		var decoradion = "${(houseLeaseResult.decoradion)!''}";//装修状况
		if(decoradion!=""){
			$("#decoradion").find("[value='"+decoradion+"']").attr("selected", "selected");
		}
		var decoradionTime = "${(houseLeaseResult.decoradionTime)!''}";//装修年限
		if(decoradionTime!=""){
			$("#decoradionTime").find("[value='"+decoradionTime+"']").attr("selected", "selected");
		}
		var payType = "${(houseLeaseResult.payType)!''}";//付款方式
		if(decoradionTime!=""){
			$("#payType").find("[value='"+payType+"']").attr("selected", "selected");
		}
		var leaseType = "${(houseLeaseResult.leaseType)!''}";//出租形式
		if(leaseType!=""){
			$("#leaseType").find("[value='"+leaseType+"']").attr("selected", "selected");
		}
		var chaoXiang = "${(houseLeaseResult.chaoXiang)!''}";//朝向
		if(chaoXiang!=""){
			$("#chaoXiang").find("[value='"+chaoXiang+"']").attr("selected", "selected");
		}
		var depositWay = "${(houseLeaseResult.depositWay)!''}";//押金要求
		if(depositWay!=""){
			$("#depositWay").find("[value='"+depositWay+"']").attr("selected", "selected");
		}
		var takeEmpty = "${(houseLeaseResult.takeEmpty)!''}";//腾空时间
		if(takeEmpty!=""){
			$("#takeEmpty").find("[value='"+takeEmpty+"']").attr("selected", "selected");
		}
		var source1 = "${(houseLeaseResult.source1)!''}";//业主来源
		if(source1!=""){
			aihome.getChildrenSource(source1);
			$("#parentSource").find("[value='"+source1+"']").attr("selected", "selected");//业主来源1
			var source2 = "${(houseLeaseResult.source2)!''}";//业主来源2
			if(source2!=""){
				$("#childSource").find("[value='"+source2+"']").attr("selected", "selected");
			}
		}
		var lookWay = "${(houseLeaseResult.lookWay)!''}";//看房方式
		if(lookWay!=""){
			var lookWayArray = lookWay.split(",");
			$(lookWayArray).each(function(i){
				$(".lookWay[value='"+lookWayArray[i]+"']").prop("checked", "checked");
			});
		}
		var fwsh = "${(houseLeaseResult.fwsh)!''}";//房屋设施
		if(fwsh!=""){
			var fwshArray = fwsh.split(",");
			$(fwshArray).each(function(i){
				$(".fwsh[value='"+fwshArray[i]+"']").prop("checked", "checked");
			});
		}
		var furniture = "${(houseLeaseResult.furniture)!''}";//室内家具
		if(furniture!=""){
			var furnitureArray = furniture.split(",");
			$(furnitureArray).each(function(i){
				$(".furniture[value='"+furnitureArray[i]+"']").prop("checked", "checked");
			});
		}
		var special = "${(houseLeaseResult.special)!''}";//房源描述
		if(special!=""){
			var specialArray = special.split(",");
			$(specialArray).each(function(i){
				$(".special[value='"+specialArray[i]+"']").prop("checked", "checked");
			});
		}
		var tags = "${(houseLeaseResult.tags)!''}";//房源标签
		if(tags!=""){
			var tagsArray = tags.split(",");
			$(tagsArray).each(function(i){
				$(".tags[value='"+tagsArray[i]+"']").prop("checked", "checked");
			});
		}

	});
	
</script>

			