<div class="col-xs-12 my-grid">
	<!-- PAGE CONTENT BEGINS -->
	<table id="jqGrid">
		
	</table>
	<div id="jqGridPager"></div>
	<!-- PAGE CONTENT ENDS -->
</div>
<script type="text/javascript">
var scripts = [null, ,null]
//var rowDataNames = [];
$('.page-content-area').ace_ajax('loadScripts', scripts, function () {});
$(function (){
	 $("#jqGrid").jqGrid({
         url: '${CTX}/demo/ajax.do',
         datatype: "json",
         colModel: [
             { label: '姓名', name: 'name',index:'name',sortable : true,width: 30 },
             { label: '年龄', name: 'age',index:'age',sortable : true, width: 50 }
             
         ],
         multiselect: true,
         //multikey: "ctrlKey",
         multiboxonly: true,
         onSelectRow: function (rowId, status, e) {
         	rowDataNames = [];
             var rowIds = jQuery("#jqGrid").jqGrid('getGridParam', 'selarrrow');
             $(rowIds).each(function(i,rowId){
                 rowDataNames.push($("#jqGrid").jqGrid('getRowData',rowId))
             });
         },
         viewrecords: true, // show the current page, data rang and total records on the toolbar
         autowidth:true,
         /* height: $(window).height()-203,
         //height: $(window).height()-108-$('.my-inputs').height(), */
         rowNum:30, 
	        rowList:[10,20,30,50,100],
         pager: "#jqGridPager",
     	 jsonReader: { root: 'pagerResults', page:'currentPage',rows:'rowCount',total:'pageCount',records:'totalCount',repeatitems: false}
     });
})
</script>