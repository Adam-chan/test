<#list (result.WT)! as item>
<input id="radioVal" sysHouseType="${(item.sysHouseType)!''}" wtId="${(item.id)!''}" type="hidden" value="${(item.sysHouseDescribe)!''}"/>
</#list>
<script type="text/javascript">
var aihome = {
	TabIdentity : "",//添加类型 4个按钮的8种类型
	title : "",//弹框title
	sysHouseDescribe : "",//往后台传的描述
	appendStr : "",//新增成功后追加的Str
	lable : "",//新增修改是lable
	ADDTYPE :"",//新增时添加参数类型
	closeFlag : "",//关闭
	updateSort : "",
	updateDescribe : "",
	updateType : "",
	updateId : "",
	getTypeParameter : function(num) {//切换TAB时获取标识
		this.TabIdentity = num;
	},
	updataEntrust : function(Type) {//前3个参数设置更改方法 Type是后台参数类型的
		var sysId = "";
		if(Type=="WT"){
			aihome.sysHouseDescribe = $(":input[name='entrustType']:checked").val();
			sysId = $(":input[name='entrustType']:checked").attr("wtId");
		}else if(Type=="YXSKQ"){
			aihome.sysHouseDescribe = $(":input[name='YXSKQType']:checked").val();
			sysId = $(":input[name='YXSKQType']:checked").attr("wtId");
		}else if(Type=="FYBH"){
			if($("#FYBHNum").val()!=""){
				aihome.sysHouseDescribe = parseInt($("#FYBHNum").val());
				sysId = $("#FYBHNum").attr("fybhId");
			}else{
				alert("请输入有效的房源添加数量");
				return;
			}
		}else if(Type=="SKIMG"){
			if($("#SKIMGNum").val()!=""){
				aihome.sysHouseDescribe = parseInt($("#SKIMGNum").val());
				sysId = $("#SKIMGNum").attr("skimgId");
			}else{
				alert("请输入有效的上传实勘图片间隔天数");
				return;
			}
		}else if(Type=="BHGSKTP"){
			if($("#YXSKQNum").val()!=""){
				aihome.sysHouseDescribe = parseInt($("#YXSKQNum").val());
				sysId = $("#YXSKQNum").attr("BHGSKTPId");
			}else{
				alert("请输入有效的优先实勘权天数");
				return;
			}
		}
		 $.ajax({
		       type: "post",
		       url: "${CTX}/sys/insertHouseInfo.do",
		       async:false,
		       dataType:'json',
		       data: {"sysHouseId":sysId,"sysHouseDescribe":aihome.sysHouseDescribe,"sysHouseType":Type},
		       success: function(data){
		    	  if(data==1){
		    		  alert("操作成功");
		    		  aihome.getSysHouseParame(Type);
		    	  }else{
		    		  alert("请注意更改的数据没有发生改变");
		    	  }
		       },
		 });
	},
	getSysHouseParame : function(Type){//查询房源参数回显
			 $.ajax({
			       type: "post",
			       url: "${CTX}/sys/getSysHouseParame.do", 
			       async:false,
			       dataType:'json',
			       data: {"sysHouseType":Type},
			       success: function(data){
			    	   aihome.getSysHouseInfo(data,Type);
			       }
			 });
	},
	deleteSysParameter : function(type){
		if (confirm("确认删除？")){
			 $.ajax({
			       type: "post",
			       url: "${CTX}/sys/deleteSysHouse.do", 
			       async:false,
			       dataType:'json',
			       data: {"sysHouseId":aihome.updateId},
			       success: function(data){
			    	   if(data!=0){
			    		   alert("删除成功");
			    		   aihome.getSysHouseParame(type);  
			    	   }
			       }
			 });
		}
	},
	getSysHouseInfo : function(data,Type){
		if(Type=="FYBH"){
			$("#houseProtectCount").val(data[0].sysHouseDescribe);
		}else if(Type=="SKIMG"){
			$("#skImgDays").val(data[0].sysHouseDescribe);
		}else {
			$("#"+Type+"Table tbody").children().remove();
			for(var i=0;i<data.length;i++){
				var appendStr = '<tr class="active"><td class="center" style="width: 20%"><label class="pos-rel"><input name="'+Type+'_Check" type="checkbox" class="ace"><span class="lbl"></span></label></td><td style="width: 20%" hidden>'+data[i].id+'</td><td style="width: 20%">'+data[i].sysHouseSort+'</td><td style="width: 60%">'+data[i].sysHouseDescribe+'</td></tr>';
				$("#"+Type+"Table tbody").append(appendStr);
			}
			$("tr.active").unbind("click").click(function (){
				if(!$(this).find(":checkbox").prop("checked")) {
					$(this).find(":checkbox").prop("checked", "checked");
					$(this).find(":checkbox").trigger("change");
				}else{
					$(this).find(":checkbox").prop("checked", "");
				}
			})
			$(":checkbox").unbind("change").change(function (){
				aihome.updateType = $(this).attr("name").split("_")[0];
				if($(this).prop("checked")) {
					$("#" + aihome.updateType + "Table :checkbox").prop("checked", "");
					$(this).prop("checked", "checked");
				}
		    });
		}
	},
	getSysParameterType : function(type,num){//查询房源参数回显
		if(type==1){ //type 是4个添加按钮
			if(this.TabIdentity==2){
				this.title = "租赁房源描述配置添加";
				this.lable = "租赁房源描述";
				this.ADDTYPE = "ZLFYMS";
			}else{
				this.title = "买卖房源描述配置添加";
				this.lable = "买卖房源描述";
				this.ADDTYPE = "MMFYMS";
			}
		}
		if(type==2){
		 	if(this.TabIdentity==2){
				this.title = "租赁房屋设施配置";
				this.lable = "租赁房屋设施";
				this.ADDTYPE = "ZLFWSS";
			}else{
				this.title = "买卖房屋设施配置";
				this.lable = "买卖房屋设施";
				this.ADDTYPE = "MMFWSS";
			}
		}
		if(type==3){
			if(this.TabIdentity==2){
				this.title = "租赁房屋室内家具配置";
				this.lable = "租赁房屋室内家";
				this.ADDTYPE = "ZLSNJJ";
			}else {
				this.title = "买卖房屋室内家具配置";
				this.lable = "买卖房屋室内家";
				this.ADDTYPE = "MMSNJJ";
			}
		}
		if(type==4){
			if(this.TabIdentity==2){
				this.title = "租赁房源标签配置";
				this.lable = "租赁房源标签";
				this.ADDTYPE = "ZLFYBQ";
			}else{
				this.title = "买卖房源标签配置";
				this.lable = "买卖房源标签";
				this.ADDTYPE = "MMFYBQ";
			}
		}
		if(type==5){
			this.title = "写字楼等级配置";
			this.lable = "写字楼等级";
			this.ADDTYPE = "XZLDJ";
		}
		if(type==6){
			this.title = "楼盘图片分类自定义配置";
			this.lable = "分类";
			this.ADDTYPE = "LPTPFL";
		}
		if(type==7){
			if(this.TabIdentity==2){
				this.title = "租赁房源看房方式配置";
				this.lable = "租赁房源看房方式";
				this.ADDTYPE = "ZLKFFS";
			}else{
				this.title = "买卖房源看房方式配置";
				this.lable = "买卖房源看房方式";
				this.ADDTYPE = "MMKFFS";
			}
		}
		if(num!=1){
				var checkObj = $(":input[name='" + aihome.ADDTYPE + "_Check']:checked");
				if(checkObj.length==0){
					alert("请选中一条数据!");
					return ;
				}else{
					aihome.updateId = checkObj.parent().parent().siblings().eq(0).text();
					aihome.updateSort = checkObj.parent().parent().siblings().eq(1).text();
					aihome.updateDescribe = checkObj.parent().parent().siblings().eq(2).text();
				}
				if(num==2){
					aihome.addSysParameter(type,num)
				}else{
					aihome.deleteSysParameter(aihome.ADDTYPE)
				}
		}else if(num==1){
			aihome.updateId = "";
			aihome.updateSort = "";
			aihome.updateDescribe = "";
			aihome.addSysParameter(type,num)
		}
	},
	addSysParameter : function(type,num) {
		bootbox.dialog({
			title:aihome.title,
			message: "<div style=\"margin-left:auto;margin-right: auto;\"class=\"row form-xs\"><label  class=\"col-xs-2 control-label\" value=\"\">排序号:</label><input maxlength=\"10\" onkeyup=\"this.value=this.value.replace(/\\D/g,'')\" onafterpaste=\"this.value=this.value.replace(/\\D/g,'')\" class=\"col-xs-3\" id=\"openSort\" name=\"\" type=\"text\" value=\"" + aihome.updateSort + "\" /><label  class=\"col-xs-3 control-label\" value=\"\">" + aihome.lable + ":</label><input maxlength=\"8\" class=\"col-xs-3\" id=\"openDescribe\" name=\"\" type=\"text\" value=\"" + aihome.updateDescribe + "\" /></div>",
		    buttons:{
		        "success" :
	             {
	                "label" : "<i class='icon-ok'></i> 保存",
	                "className" : "btn-sm btn-success",
	                "callback": function() {
	                	var	openDescribe ="";
	                    var	openSort = "";
		                openDescribe = $("#openDescribe").val();
		                openSort = $("#openSort").val();
		                aihome.closeFlag = false;
		                if(openDescribe!=""&&openSort!=""){
	                	  $.ajax({
		                        type: "post",
		                        dataType: "json",
		                        url: "${CTX}/sys/insertHouseInfo.do",
		                        data:{"sysHouseId":aihome.updateId,"updateSort":aihome.updateSort,"updateDescribe":aihome.updateDescribe,"sysHouseDescribe":openDescribe,"sysHouseSort":openSort,"sysHouseType":aihome.ADDTYPE,"operationType":num},
		                        async: false,
		                        success: function(data){
		                            if (data==2){
		                            	alert("输入的"+aihome.lable+"已存在");
		                            }else if(data==3){
		                            	alert("输入的排序号已存在,请输入其它的排序号");
		                            }else{
		                            	if(num==1){
		                            		alert("保存成功");
		                            		aihome.getSysHouseParame(aihome.ADDTYPE);
		                            	}else{
		                            		alert("修改成功");
		                            		aihome.getSysHouseParame(aihome.ADDTYPE);
		                            	}
		                            	$(":input[name='" + aihome.ADDTYPE + "_Check']:checked").prop("checked", "");
		                            	aihome.closeFlag = true;
		                            }
		                        }
		                  });
		                }else{
		                	alert("请填写未填写内容!");
		                }
		                return aihome.closeFlag;
	                }
	            }
		    },
		});
	}
};
$(function(){
	$('.page-content-area').ace_ajax('loadScripts', [null,"../assets/js/bootbox.js"], function () {});
	$(".tab-content.no-padding").css({"width": "100%", "height": "227px"});
	$(":radio[name='entrustType'][value='" + $("#radioVal").val()+ "']").prop("checked","checked");
	$("tr.active").click(function (){
		if(!$(this).find(":checkbox").prop("checked")) {
			$(this).find(":checkbox").prop("checked", "checked");
			$(this).find(":checkbox").trigger("change");
		}else{
			$(this).find(":checkbox").prop("checked", "");
		}
	})
	$(":checkbox").change(function (){
		aihome.updateType = $(this).attr("name").split("_")[0];
		if($(this).prop("checked")) {
			$("#" + aihome.updateType + "Table :checkbox").prop("checked", "");
			$(this).prop("checked", "checked");
		}
    });
})
</script>