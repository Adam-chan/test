aihome.saveCustomer = function(){
	if($("#customerId").val() != null && $("#customerId").val() != ""){
		return $("#customerId").val();
	}
	var lastId = "";
	$.ajax({
		type : "post",
		url : "${CTX}/customerManage/addCustomer.do",
		async :false,
		dataType :'json',
		data : {
			"id" : $("#customerId").val(),
			"tell" : $("#tel").val(),
			"userName" : $("#customerName").val(),
			"isStaff" : aihome.isStaff, 
			"userType" : $("[id^='userType']:checked").val(),
			"spareTell" : $("#spareTell").val(),
			"sex" : $("[id^='sex']:checked").val(),
			"idType" : $("#idType").val(),
			"idCode" : $("#identityCode").val(),
			"country" : $("#country").val(),
			"censusRegister" : $("#huji").val(),
			"pubFundsBlance" : $("#pubFundsBlance").val(),
			"pubFundsMonth" : $("#pubFundsMonth").val(),
			"userAge" : $("#userAge").val(),
			"buyExperience" : $("#buyExperience").val(),
			"loanRecord" : $("#loanRecord").val(),
			"kaiShou" : $("[id^='kaiShou']:checked").val(),
			"viewHome" : $("[id^='viewHome']:checked").val(),
			"presentAddress" : $("#presentAddress").val(),
			"censusRegisterAddress" : $("#censusRegisterAddress").val(),
			"company" : $("#company").val(),
			"companyAddress" : $("#companyAddress").val(),
			"remark" : $("#remark").val()
			},
		success : function(data){
			if(data == 1) {
				alert("保存成功");
			}else {
				alert("保存失败");
			}
		},
	 });
	$.ajax({
		type : "post",
		url : "${CTX}/customerManage/getLastInsertCustomerId.do",
		async :false,
		dataType :'json',
		success : function(data){
			lastId = data;
		},
	 });
	return lastId;
}

