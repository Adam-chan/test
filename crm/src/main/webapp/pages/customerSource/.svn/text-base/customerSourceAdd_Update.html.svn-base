<script type="text/javascript">

aihome.fillAddForm = function(){
	//修改默认选中框
	$(":radio[name='userType'][value='" + $("#userType_Radio").val() + "']").prop("checked", "checked");
	$(":radio[name='sex'][value='" + $("#sex_Radio").val() + "']").prop("checked", "checked");
	$(":radio[name='kaiShou'][value='" + $("#kaiShou_Radio").val() + "']").prop("checked", "checked");
	$(":radio[name='cust_viewHome'][value='" + $("#cust_viewHome_Radio").val() + "']").prop("checked", "checked");
	$("#buyExperience").val($("#buyExperience_Sel").val());
	$("#loanRecord").val($("#loanRecord_Sel").val());
	$("#idType").val($("#idType_Sel").val());
	$("#country").val($("#country_Sel").val());
	if($("#isStaff_Check").val()==1){
		$(":checkbox[name='isStaff']").prop("checked", "");
	}else if($("#isStaff_Check").val()==2){
		$(":checkbox[name='isStaff']").prop("checked", "checked");
	}
}

aihome.isStaff = $("#isStaff_Check").val();
$("[id^='isStaff']").click(function() {
	if($(this).prop("checked")) {
		aihome.isStaff = 2;
	}else{
		aihome.isStaff = 1;
	}
});


