/* 关闭SPAN按钮 */
closeSpan = function(obj) {
	var id = $(obj).parent().parent().attr("id");
	var objs = $("[id^='" + $(obj).parent().next().attr("id") + "'][value='" + $(obj).parent().next().val() +"']");
	objs.prev().remove();
	objs.next().remove();
	objs.remove();
	if(id.indexOf("demandPremisesLabel") != -1) {
		aihome.countDemandPremises();
	} else if(id.indexOf("otherDemandRegionLabel") != -1) {
		aihome.countOtherDemandRegion();
	} 
}

aihome.countOtherDemandRegion = function() {
	aihome.otherDemandRegionIdsStr = "";
	var otherDemandRegionIds = $("#addPage").find("[id='otherDemandRegionLabel']:visible").find("[id='otherDemandRegionIds']");
	for(var i = 0; i < otherDemandRegionIds.length; i++) {
		aihome.otherDemandRegionIdsStr += otherDemandRegionIds[i].value + ",";
	}
	aihome.otherDemandRegionIdsStr = aihome.otherDemandRegionIdsStr.substring(0,aihome.otherDemandRegionIdsStr.length-1);
	aihome.otherDemandRegionNamesStr = "";
	var otherDemandRegionNames = $("#addPage").find("[id='otherDemandRegionLabel']:visible").find("[id='otherDemandRegionNames']");
	for(var i = 0; i < otherDemandRegionNames.length; i++) {
